{
  "name": "js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "travis": "npm build && npm test && npm run nyc-coverage",
    "test": "xo --fix && nyc ava",
    "nyc-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "BABEL_ENV=production babel --out-dir=dist index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "jsinspect": "^0.12.6",
    "nyc": "^11.0.3",
    "xo": "^0.18.2"
  },
  "ava": {
    "babel": "inherit",
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "files": [
      "linked-list/**/*.test.js"
    ],
    "modules": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "source": [
      "linked-list/**/*.js"
    ],
    "tap": true
  },
  "xo": {
    "space": true,
    "fix": true
  },
  "dependencies": {}
}
